    <tr>
        <td>
            <a id="<%= $Id %>">
                <%= $Name %>
            </a>
        </td>
        <td><%= $Location %></td>
        <td><%= $ResourceGroupName %></td>
        <td><%= $Sku %></td>
        <td><%= $Version %></td>
        <td><%= $AllocationMethod %></td>
        <td><%= $IpAddress %></td>
        <td><%= ConvertFromIdtoLink $IpConfiguration "IpConfigurations" %></td>

        <td>
            <% if ( $DnsSettings -ne $null ) { -%>
            <table class="table table-sm table-bordered table-hover" style="table-layout: fixed;">
                <colgroup>
                    <col style="width:130px;">
                    <col style="width:300px;">
                </colgroup>
                <tbody>
                    <tr>
                        <th>DomainNameLabel</th>
                        <td><%= $DnsSettings.DomainNameLabel %></td>
                    </tr>
                    <tr>
                        <th>Fqdn</th>
                        <td><%= $DnsSettings.Fqdn %></td>
                    </tr>
                </tbody>
            </table>
            <% } -%>
        </td>

        
    </tr>