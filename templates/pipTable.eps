    <tr>
        <td>
            <a id="<%= $Id %>">
                <%= $Name %>
            </a>
        </td>
        <td><%= $Location %></td>
        <td><%= $ResourceGroupName %></td>
        <td><%= $Sku %></td>
        <td><%= $Version %></td>
        <td><%= $AllocationMethod %></td>
        <td><%= $IpAddress %></td>

        <td>
            <% if ( $IpConfiguration -ne $null ) { -%>
                <% $IpConfiguration -Match "IpConfigurations/(.*)?" -%>
                <a href="#<%= $IpConfiguration.ToLower() %>">
                    <%= $Matches[1] %>
                </a>
            <% } -%>
        </td>                

        <td>
            <% if ( $DnsSettings -ne $null ) { -%>
            <table>
                <tbody>
                    <tr>
                        <th>DomainNameLabel</th>
                        <td><%= $DnsSettings.DomainNameLabel %></td>
                    </tr>
                    <tr>
                        <th>Fqdn</th>
                        <td><%= $DnsSettings.Fqdn %></td>
                    </tr>
                </tbody>
            </table>
            <% } -%>
        </td>

        
    </tr>