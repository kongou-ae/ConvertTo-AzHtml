    <tr>
        <td><%= $Name %></td>
        <td><%= $Location %></td>
        <td><%= $ResourceGroupName %></td>
        
        <td>
            <ul class="list-unstyled">
                <% $AddressSpace | each { -%> 
                    <li><%= $_ %></li>
                <% } -%>
            </ul>
        </td>
        
        <td>
            <table class="table table-sm table-bordered" style="table-layout: fixed;">
                <colgroup>
                    <col style="width:100px;">
                    <col style="width:100px;">
                    <col style="width:100px;">
                    <col style="width:100px;">
                </colgroup>
                <thead class="thead-dark">
                    <tr>
                        <th>Name</th>
                        <th>AddressPrefix</th>
                        <th>NetworkSecurityGroup</th>
                        <th>RouteTable</th>
                    </tr>
                </thead>
                <tbody>
                <% $Subnets | each { -%> 
                    <tr>
                        <td>
                            <a id="<%= $_.Id.ToLower() %>">
                                <%= $_.Name %>
                            </a>
                        </td>
                        <td><%= $_.AddressPrefix %></td>
                        
                        <td><%= ConvertFromIdtoLink $_.NetworkSecurityGroup.Id "networkSecurityGroups" %></td>

                        <td><%= ConvertFromIdtoLink $_.RouteTable.Id "RouteTables" %></td>

                    </tr>
                <% } -%>
                </tbody>
            </table>
        </td>
    </tr>