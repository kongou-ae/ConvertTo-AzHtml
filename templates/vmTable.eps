    <tr>
        <td>
            <a id="<%= $Id %>">
                <%= $Name %>
            </a>
        </td>
        <td><%= $Location %></td>
        <td><%= $ResourceGroupName %></td>
        <td><%= $VmSize %></td>
        <td><%= $OsType %></td>

        <td>
            <% if ( $imageReference -ne $null ) { -%>
            <table>
                <tbody>
                    <tr>
                        <th>Publisher</th>
                        <td><%= $imageReference.Publisher %></td>
                    </tr>
                    <tr>
                        <th>Offer</th>
                        <td><%= $imageReference.Offer %></td>
                    </tr>
                    <tr>
                        <th>Sku</th>
                        <td><%= $imageReference.Sku %></td>
                    </tr>
                    <tr>
                        <th>ExactVersion</th>
                        <td><%= $imageReference.ExactVersion %></td>
                    </tr>
                </tbody>
            </table>
            <% } -%>
        </td>

        <td>
            <table>
                <tbody>
                    <tr>
                        <th>ManagedDisk</th>
                        <% $OsDisk.ManagedDisk.Id -Match "disks/(.*)?" -%>
                        <td>
                            <a href="#<%= $OsDisk.ManagedDisk.Id.ToLower() %>">
                                <%= $Matches[1] %>
                            </a>
                        </td>
                    </tr>
                    <tr>
                        <th>Caching</th>
                        <td><%= $OsDisk.Caching %></td>
                    </tr>
                </tbody>
            </table>
        </td>

        <td>
            <% if ( $DataDisks.count -ne 0 ) { %>
            <table class="table table-sm table-bordered">
                <thead class="thead-dark">
                    <tr>
                        <th>Lun</th>
                        <th>Name</th>
                        <th>Caching</th>
                    </tr>
                </thead>                
                <% $DataDisks | each { -%>
                <tbody>
                    <td><%= $_.Lun %></td>
                    <% $_.ManagedDisk.Id -Match "Microsoft.Compute/disks/(.*)?" -%>
                        <td>
                            <a href="#<%= $_.ManagedDisk.Id.ToLower() %>">
                                <%= $Matches[1] %>
                            </a>
                        </td>
                    <td><%= $_.Caching %></td>
                </tbody>
                <% } -%>
            </table>
            <% } -%>
        </td>

        <td>
            <ul class="list-unstyled">
            <% $NetworkInterfaces | each { -%> 
                <% $_.Id -Match "NetworkInterfaces/(.*)?" -%>
                    <a href="#<%= $_.Id.ToLower() %>">
                        <li><%= $Matches[1] %></li>
                    </a>
            <% } -%>
            </ul>
        </td>
    </tr>