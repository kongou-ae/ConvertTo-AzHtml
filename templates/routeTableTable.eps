    <tr>
        <td>
            <a id="<%= $Id %>">
                <%= $Name %>
            </a>
        </td>
        <td><%= $Location %></td>
        <td><%= $ResourceGroupName %></td>
        
        <td>
            <% if ( $Routes.count -ne 0 ) { -%>
            <table class="table table-sm table-bordered table-hover" style="table-layout: fixed;">
                <colgroup>
                    <col style="width:100px;">
                    <col style="width:100px;">
                    <col style="width:100px;">
                    <col style="width:100px;">
                <thead class="thead-dark">
                    <tr>
                        <th>Name</th>
                        <th>AddressPrefix</th>
                        <th>NextHopType</th>
                        <th>NextHopIpAddress</th>
                    </tr>
                </thead>
                <tbody>
                    <% $Routes | each { -%>
                        <tr>
                            <td><%= $_.Name %></td>
                            <td><%= $_.AddressPrefix %></td>
                            <td><%= $_.NextHopType %></td>
                            <td><%= $_.NextHopIpAddress %></td>
                        </tr>
                    <% } -%>
                </tbody>

            <% } -%>
            </table>
        </td>
    </tr>