    <tr>
        <td>
            <a id="<%= $Id %>">
                <%= $Name %>
            </a>
        </td>
        <td><%= $Location %></td>
        <td><%= $ResourceGroupName %></td>
        <td>
            <table class="table table-sm table-bordered" style="table-layout: fixed;">
                <colgroup>
                    <col style="width:70px;">
                    <col style="width:120px;">
                    <col style="width:60px;">
                    <col style="width:120px;">
                    <col style="width:110px;">
                </colgroup>
                <thead class="thead-dark">
                    <tr>
                        <th>Name</th>
                        <th>PrivateIpAddress</th>
                        <th>Version</th>
                        <th>AllocationMethod</th>
                        <th>PublicIpAddress</th>                        
                    </tr>
                </thead>                
            
                <% $IpConfigurations | each { -%>
                    <tbody>
                        <td>
                            <a id="<%= $_.Id.ToLower() %>">
                                <%= $_.Name %>
                            </a>
                        </td>
                        <td><%= $_.PrivateIpAddress %></td>
                        <td><%= $_.PrivateIpAddressVersion %></td>
                        <td><%= $_.PrivateIpAllocationMethod %></td>
                        <td><%= ConvertFromIdtoLink $_.PublicIpAddress.Id "publicIPAddresses" %></td>
                    </tbody>
                <% } -%>
            </table>
        </td>
        <td>
            <ul class="list-unstyled">
                <% $DnsSettings.DnsServers | each { -%> 
                    <li><%= $_ %></li>
                <% } -%>
            </ul>
        </td>
        <td><%= $EnableIPForwarding %></td>
        <td><%= ConvertFromIdtoLink $NetworkSecurityGroup.Id "networkSecurityGroups" %></td>
    </tr>