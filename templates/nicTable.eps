    <tr>
        <td>
            <a id="<%= $Id %>">
                <%= $Name %>
            </a>
        </td>
        <td><%= $Location %></td>
        <td><%= $ResourceGroupName %></td>
        <td>
            <table class="table table-sm table-bordered" style="table-layout: fixed;">
                <colgroup>
                    <col style="width:70px;">
                    <col style="width:120px;">
                    <col style="width:60px;">
                    <col style="width:120px;">
                    <col style="width:110px;">
                </colgroup>
                <thead class="thead-dark">
                    <tr>
                        <th>Name</th>
                        <th>PrivateIpAddress</th>
                        <th>Version</th>
                        <th>AllocationMethod</th>
                        <th>PublicIpAddress</th>                        
                    </tr>
                </thead>                
            
                <% $IpConfigurations | each { -%>
                    <tbody>
                        <td>
                            <a id="<%= $_.Id.ToLower() %>">
                                <%= $_.Name %>
                            </a>
                        </td>
                        <td><%= $_.PrivateIpAddress %></td>
                        <td><%= $_.PrivateIpAddressVersion %></td>
                        <td><%= $_.PrivateIpAllocationMethod %></td>

                        <td>
                            <% if ( $_.PublicIpAddress -ne $null ) { -%>
                                <% $_.PublicIpAddress.Id -Match "/publicIPAddresses/(.*)?" -%>
                                <a href="#<%= $_.PublicIpAddress.Id.ToLower() %>">
                                    <%= $Matches[1] %>
                                </a>
                            <% } -%>
                        </td>

                    </tbody>
                <% } -%>
            </table>
        </td>
        <td>
            <ul class="list-unstyled">
                <% $DnsSettings.DnsServers | each { -%> 
                    <li><%= $_ %></li>
                <% } -%>
            </ul>
        </td>
        <td><%= $EnableIPForwarding %></td>
        <td>
            <% if ( $NetworkSecurityGroup -ne $null ) { -%>
                <% $NetworkSecurityGroup.Id -Match "networkSecurityGroups/(.*)?" -%>
                <a href="#<%= $NetworkSecurityGroup.Id.ToLower() %>">
                    <%= $Matches[1] %>
                </a>
            <% } -%>
        </td>

    </tr>